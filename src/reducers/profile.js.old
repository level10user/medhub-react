import { PROFILE_FETCHED } from "../types";

export default function profiles(state = {}, action = {}) {
  switch (action.type) {
    case PROFILE_FETCHED:
      return { ...state, ...action.data.profile };
    default:
      return state;
  }
}

// SELECTORS

